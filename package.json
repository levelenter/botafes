{
  "scripts": {
    "dev:web": "webpack -w",
    "dev:sv": "ts-node ./src/server/index.ts",
    "prod": "run-s prod:web prod:tsc prod:sv",
    "prod:web": "webpack",
    "prod:sv": "pm2 start config/pm2.config.json --env production",
    "prod:tsc": "tsc",
    "serve": "pm2 start",
    "seed": "npx prisma db seed",
    "_seed": "ts-node ./src/data/seed.ts --project tsconfig.seed.json",
    "studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate dev --name migrate",
    "start": "docker-compose up --build"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/data/seed.ts"
  },
