    "npm-run-all": "^4.1.5",
    "prisma": "^4.4.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "private": true,
  "dependencies": {
